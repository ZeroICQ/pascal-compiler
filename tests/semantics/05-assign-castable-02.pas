var
    a, b : Integer;
    c, d : float;
    e, f : char;
begin
    a := b;
    a += b;
    a -= b;
    a *= b;
    //a /= b;
    
    c := d;
    c += d;
    c -= d;
    c *= d;
    c /= d;
    
    e := f;
    e += f;
    e -= f;
    e *= f;
    //e /= f;
    
    //float-integer
    c := a;
    c += a;
    c -= a;
    c *= a;
end.